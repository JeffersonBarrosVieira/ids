{
    "version": 2,
    "builds":[
        {
            "src":"app.py",
            "use":"@vercel/python"
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"app.py"
        }
    ],
    "functions": {
        "static/js/main.js": {
          "maxDuration": 5
        },
        "api/*.py": {
          "maxDuration": 5
        }
      }
}