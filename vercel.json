{
    "version": 2,
    "builds":[
        {
            "src":"app.py",
            "use":"@vercel/python"
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"app.py"
        }
    ],
    "functions": {
        "api/calc.py": {
          "maxDuration": 30
        },
        "api/*.py": {
          "maxDuration": 15
        }
      }
}